#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/user/Desktop/Foma/medikey/node_modules/.pnpm/vitest@3.2.4_@types+node@22_3a13f6c548398a5abdf4d6669f27bcb5/node_modules/vitest/node_modules:/mnt/c/Users/user/Desktop/Foma/medikey/node_modules/.pnpm/vitest@3.2.4_@types+node@22_3a13f6c548398a5abdf4d6669f27bcb5/node_modules:/mnt/c/Users/user/Desktop/Foma/medikey/node_modules/.pnpm/node_modules:/mnt/c/Users/user/Desktop/Foma/medikey/node_modules:/mnt/c/Users/user/Desktop/Foma/node_modules:/mnt/c/Users/user/Desktop/node_modules:/mnt/c/Users/user/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/user/desktop/foma/medikey/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/user/Desktop/Foma/medikey/node_modules/.pnpm/vitest@3.2.4_@types+node@22_3a13f6c548398a5abdf4d6669f27bcb5/node_modules/vitest/node_modules:/mnt/c/Users/user/Desktop/Foma/medikey/node_modules/.pnpm/vitest@3.2.4_@types+node@22_3a13f6c548398a5abdf4d6669f27bcb5/node_modules:/mnt/c/Users/user/Desktop/Foma/medikey/node_modules/.pnpm/node_modules:/mnt/c/Users/user/Desktop/Foma/medikey/node_modules:/mnt/c/Users/user/Desktop/Foma/node_modules:/mnt/c/Users/user/Desktop/node_modules:/mnt/c/Users/user/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/user/desktop/foma/medikey/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../vitest/vitest.mjs" "$@"
fi
